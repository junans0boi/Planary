  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "electron:dev": "concurrently \"npm:electron:serve\" \"npm run electron:start\"",
